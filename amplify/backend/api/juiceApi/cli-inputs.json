{
  "version": 1,
  "paths": {
    "/promos": {
      "name": "/promos",
      "lambdaFunction": "promoLambda",
      "permissions": {
        "setting": "protected",
        "auth": [
          "read",
          "delete"
        ],
        "guest": [
          "create",
          "update"
        ]
      }
    },
    "/contactus": {
      "name": "/contactus",
      "lambdaFunction": "contactLambda",
      "permissions": {
        "setting": "protected",
        "auth": [
          "read",
          "delete"
        ],
        "guest": [
          "create"
        ]
      }
    }
  }
}